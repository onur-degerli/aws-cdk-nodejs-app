{
  "name": "aws-cdk-nodejs",
  "version": "0.1.0",
  "bin": {
    "aws-cdk-nodejs": "bin/aws-cdk-nodejs.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "npx jest --passWithNoTests",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "cdk": "cdk",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "deploy": "cdk deploy --all",
    "deploy-force": "cdk deploy --all --require-approval never",
    "destroy": "cdk destroy --all",
    "docker:build": "docker-compose down -v && docker-compose up --build",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "npm test"
    ]
  },
  "devDependencies": {
    "@eslint/css": "^0.13.0",
    "@eslint/js": "^9.38.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.4.1",
    "@types/aws-lambda": "^8.10.156",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.11",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "aws-cdk": "2.1029.4",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.46.2"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.916.0",
    "aws-cdk-lib": "^2.220.0",
    "constructs": "^10.4.2"
  }
}
